{
  "name": "nextuber",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.15.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint": "next lint",
    "start": "next start -p ${PORT:=3000}",
    "test": "NODE_ENV=test jest --watchAll --verbose",
    "build:css": "postcss assets/main.css -o assets/main_browser.css",
    "codegen:generate": "npx apollo client:codegen --target=typescript --outputFlat",
    "lint:fix": "eslint \"components/**/*.{js,jsx,ts,tsx}\" --fix --ext tsx",
    "format": "prettier --write  \"components/**/*.{js,jsx,ts,tsx}\" --config ./.prettierrc"
  },
  "dependencies": {
    "@apollo/client": "3.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.2.2",
    "@types/jest": "^26.0.15",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@apollo/react-ssr": "^4.0.0",
    "apollo-upload-client": "^14.1.3",
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^1.0.4",
    "@types/marked": "^4.0.3",
    "body-scroll-lock": "^3.1.5",
    "classnames": "^2.3.1",
    "js-cookie": "^2.2.1",
    "keen-slider": "^5.5.1",
    "gray-matter": "^4.0.3",
    "marked": "^4.0.18",
    "next": "13.3.1",
    "next-with-apollo": "^5.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^6.11.4",
    "react-ticker": "^1.2.2",
    "sass": "^1.38.2",
    "swiper": "^6.7.5",
    "swr": "^1.2.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "./components/**/*.tsx",
      "./pages/**/*.tsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "components"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.3.3",
    "@testing-library/cypress": "^7.0.2",
    "jest": "^26.6.3",
    "mock-apollo-client": "^1.0.0",
    "@svgr/webpack": "^5.5.0",
    "@types/body-scroll-lock": "^2.6.1",
    "@types/js-cookie": "^2.2.6",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "autoprefixer": "^10.4.7",
    "@types/apollo-upload-client": "^17.0.2",
    "deepmerge": "^4.2.2",
    "eslint-config-next": "12.2.2",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "postcss-nesting": "^8.0.1",
    "stylelint-config-standard": "^29.0.0",
    "tailwindcss": "^2.1.4",
    "typescript": "4.7.4"
  }
}